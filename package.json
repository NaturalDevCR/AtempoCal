{
  "name": "atempo-cal",
  "version": "0.1.9",
  "private": false,
  "description": "A flexible and resource-centric calendar component for Vue 3, designed for planning and scheduling tasks.",
  "author": "NaturalDevCR<jdavidoa91@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/NaturalDevCR/atempo-cal#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NaturalDevCR/atempo-cal.git"
  },
  "bugs": {
    "url": "https://github.com/NaturalDevCR/atempo-cal/issues"
  },
  "keywords": [
    "vue",
    "vue3",
    "component",
    "calendar",
    "scheduler",
    "resource planner",
    "atemporal"
  ],
  "type": "module",
  "main": "./dist/atempo-cal.umd.js",
