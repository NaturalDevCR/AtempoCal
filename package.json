{
  "name": "atempo-cal",
  "version": "2.0.0",
  "description": "A Vue 3 weekly scheduling calendar component designed for worker/resource management with dark/light theme support and TypeScript integration",
  "type": "module",
  "main": "./dist/atempo-cal.umd.js",
  "module": "./dist/atempo-cal.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/atempo-cal.es.js",
      "require": "./dist/atempo-cal.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/atempo-cal.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cd demo && vite",
    "dev:demo": "cd demo && vite",
    "build": "vue-tsc --skipLibCheck && vite build",
    "build:demo": "cd demo && vite build",
    "preview": "cd demo && vite preview",
    "preview:demo": "cd demo && vite preview",
    "check": "vue-tsc --noEmit && eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "prepublishOnly": "npm run check && npm run build",
    "release": "standard-version",
    "publish:npm": "npm publish --access public"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "atemporal": "^0.1.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.12.0",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "postcss": "^8.4.32",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.0",
    "tsup": "^8.5.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.9.1",
    "vue-tsc": "^1.8.25"
  },
  "peerDependencies": {
    "atemporal": "^0.1.0",
    "vue": "^3.4.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "calendar",
    "scheduler",
    "weekly-calendar",
    "resource-scheduling",
    "worker-scheduling",
    "atemporal",
    "typescript",
    "tailwind",
    "dark-theme",
    "light-theme",
    "scheduling-component",
    "employee-scheduling",
    "shift-planning"
  ],
  "author": "Josue Orozco A.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NaturalDevCR/AtempoCal.git"
  },
  "bugs": {
    "url": "https://github.com/NaturalDevCR/AtempoCal/issues"
  },
  "homepage": "https://github.com/NaturalDevCR/AtempoCal#readme"
}
