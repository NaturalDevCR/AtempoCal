    "release": "npm run build && npm version patch",
    "postversion": "npm publish"
  },
  "dependencies": {
    "atemporal": "^0.1.0",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.4.5",
