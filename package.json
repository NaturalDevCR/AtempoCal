{
  "name": "atempo-cal",
  "version": "1.0.0",
  "description": "A Vue 3 weekly scheduling calendar component designed for workers scheduling management",
  "type": "module",
  "main": "./dist/atempo-cal.umd.js",
  "module": "./dist/atempo-cal.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/atempo-cal.es.js",
      "require": "./dist/atempo-cal.umd.js"
    },
    "./dist/style.css": "./dist/atempo-cal.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cd demo && vite",
    "dev:demo": "cd demo && vite",
    "build": "vue-tsc --skipLibCheck && vite build",
    "build:demo": "cd demo && vite build",
    "preview": "cd demo && vite preview",
    "preview:demo": "cd demo && vite preview",
    "check": "vue-tsc --noEmit && eslint .",
    "lint": "eslint . --fix",
    "type-check": "vue-tsc --noEmit",
    "prepublishOnly": "npm run check && npm run build",
    "release": "standard-version",
    "publish:npm": "npm publish --access public"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "@tailwindcss/postcss": "^4.0.0",
    "atemporal": "0.1.9",
    "tailwindcss": "^4.0.0",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.12.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/tsconfig": "0.8.1",
    "autoprefixer": "10.4.21",
    "eslint": "9.36.0",
    "eslint-plugin-vue": "10.5.0",
    "postcss": "8.5.6",
    "standard-version": "9.5.0",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "vite": "7.1.7",
    "vite-plugin-dts": "4.5.4",
    "vue-tsc": "3.0.8"
  },
  "peerDependencies": {
    "atemporal": "0.1.9",
    "vue": "^3.5.22"
  },
  "keywords": [
    "vue",
    "vue3",
    "calendar",
    "scheduler",
    "weekly-calendar",
    "resource-scheduling",
    "worker-scheduling",
    "atemporal",
    "typescript",
    "tailwind",
    "dark-theme",
    "light-theme",
    "scheduling-component",
    "employee-scheduling",
    "shift-planning"
  ],
  "author": "Josue Orozco A.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NaturalDevCR/AtempoCal.git"
  },
  "bugs": {
    "url": "https://github.com/NaturalDevCR/AtempoCal/issues"
  },
  "homepage": "https://github.com/NaturalDevCR/AtempoCal#readme"
}
